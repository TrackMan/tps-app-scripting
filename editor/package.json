{
  "name": "editor",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "codegen": "graphql-codegen --config codegen.yml",
    "codegen:watch": "chokidar \"src/**/*.{ts,tsx}\" -c \"npm run codegen\" --initial",
    "dev:full": "concurrently \"npm run dev\" \"npm run codegen:watch\""
  },
  "dependencies": {
    "@monaco-editor/react": "^4.7.0",
    "@types/marked": "^5.0.2",
    "@urql/exchange-auth": "^3.0.0",
    "@urql/exchange-graphcache": "^8.1.0",
    "ajv": "^8.17.1",
    "graphql": "^16.11.0",
    "marked": "^16.3.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "urql": "^5.0.1",
    "wonka": "^6.3.5"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^6.0.0",
    "@graphql-codegen/typescript": "^5.0.0",
    "@graphql-codegen/typescript-operations": "^5.0.0",
    "@graphql-codegen/typescript-urql": "^4.0.1",
    "@parcel/watcher": "^2.5.1",
    "@rollup/rollup-win32-x64-msvc": "^4.50.2",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.0",
    "chokidar": "^4.0.3",
    "concurrently": "^9.2.1",
    "rollup": "^4.50.2",
    "typescript": "^5.0.0",
    "vite": "^7.0.0"
  }
}
